[comment encoding = UTF-8 /]
[module mutableBean('http://www.eclipse.org/uml2/4.0.0/UML')]
[import org::eclipse::acceleo::module::sample::common::genutils /]
[import org::eclipse::acceleo::module::sample::common::propertyGetter /]

[template public mutableBean(class : Class)]
[comment @main /]
[file (class.name + '.java', false)]
[if (class.hasComment())]
/**
 * [class.getComment()/]"
 *
 * @return [class.getComment()/]
 */	
[/if]
public class [class.name/] {

[for (property : Property | class.ownedAttribute)]
	private [property.typer()/] [property.name/]; 
[/for]

[for (property : Property | class.ownedAttribute)]
	 [propertyGetter(property)/]
[/for]
		
}	
[/file]
[/template]
